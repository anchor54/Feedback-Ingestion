  "scripts": {
    "test": "jest",
    "build": "tsc",
    "start": "node dist/services/webhook/server.js",
    "dev:webhook": "nodemon src/services/webhook/server.ts",
    "dev:processor": "nodemon src/services/processing/processor.ts",
    "dev:polling": "nodemon src/services/polling/pollingService.ts",
    "test:webhook": "npm run build && node test-webhook-sender.js",
    "test:webhook:interactive": "npm run build && node test-webhooks-interactive.js",
    "test:webhook:single": "npm run build && node test-webhook-sender.js single",
    "test:webhook:burst": "npm run build && node test-webhook-sender.js burst",
    "test:webhook:continuous": "npm run build && node test-webhook-sender.js start",
    "docker:start": "./docker-start.sh",
    "docker:stop": "./docker-stop.sh",
    "docker:dev": "docker compose -f docker-compose.dev.yml up -d",
    "docker:dev:stop": "docker compose -f docker-compose.dev.yml down",
    "docker:build": "docker compose build",
    "docker:logs": "docker compose logs -f",
    "docker:logs:webhook": "docker compose logs -f webhook-service",
    "docker:logs:processor": "docker compose logs -f processing-service",
    "docker:logs:polling": "docker compose logs -f polling-service"
